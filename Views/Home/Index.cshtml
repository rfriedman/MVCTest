﻿@model IEnumerable<TablePage.Models.Customer>
@*
    @{
        ViewBag.Title = "Index";
    }
*@

<h2>Index</h2>
<div class="main-content" >
        
    <table id="tblCust">
        <thead>
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Phone</th>
            <th>Birthday</th>
            <th>City</th>
            <th>Country</th>
            <th>Email</th>
            <th>Region</th>
            <th>Street</th>
        </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.id</td>
                    <td>@item.first_name</td>
                    <td>@item.last_name</td>
                    <td>@item.phone</td>
                    <td>@item.dob</td>
                    <td>@item.city</td>
                    <td>@item.country</td>
                    <td>@item.email</td>
                    <td>@item.region</td>
                    <td>@item.street</td>
                </tr>
            }
        </tbody>
    </table>
</div>